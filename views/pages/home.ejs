<!doctype html>
<html lang="en">
  <%- include('../partials/head'); %>
  <body class="min-w-96">
    <header><%- include('../partials/nav'); %></header>
    <main>
      <section class="text-center m-auto w-11/12 md:w-1/2 pt-16">
        <div>
          <h2 class="text-2xl py-2 font-bold">Welcome Back <%= user.name %></h2>
        </div>
      </section>
      <section>
       
        <div class="flex gap-6 flex-wrap w-11/12 md:w-10/12 m-auto justify-center sm:justify-center">
          <%- include('../partials/gallery-header'); %>
          <% photos.forEach(function(photo, i){ %>
          <div class="p-1 self-start bg-[<%= photo.hex%>]">
            <img class="sm:max-w-80 sm:max-h-80 sm:min-w-52 sm:min-h-52" loading="lazy" src="/api/v1/photos/<%= photo.photoId %>">
          </div>
          <% }); %>
        </div>
      </section>
    </main>
    <%- include('../partials/footer'); %>
  </body>
</html>
